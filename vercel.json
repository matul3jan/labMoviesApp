{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tmdb-client-api.onrender.com/:path*"
    },
    {
      "source": "/proxy/:match*",
      "destination": "https://tmdb-client-api.onrender.com/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}